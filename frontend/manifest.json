{
    "name": "Collate",
    "version": "0.0.1",
    "description": "This is the full description of the plugin",
    "manifest_version": 2,
    "browser_action": {
      "default_popup": "./index.html",
      "default_title": "Open the popup"
    },
    "content_scripts": [
        {
            "js": ["trial.js"],
            "matches": ["<all_urls>"]
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions" : ["tabs"],
    "icons": {
        "128" : "favicon.png"
    }
}